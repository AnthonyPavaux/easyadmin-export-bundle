{
  "name": "AnthonyPavaux/easyadmin-export-bundle",
  "description": "Generate spreadsheets for entities managed by the EasyAdmin.",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Anthony Pavaux",
    }
  ],
  "require": {
    "php": "^8.0.3",
    "doctrine/collections": "^1.5",
    "doctrine/common": "^2.8",
    "doctrine/doctrine-bundle": "^1.8",
    "doctrine/orm": "^2.6",
    "easycorp/easyadmin-bundle": "^2.0",
    "symfony/framework-bundle": "^4.1",
    "symfony/http-foundation": "^4.1",
    "symfony/http-kernel": "^4.1",
    "symfony/property-access": "^4.1",
    "phpoffice/phpspreadsheet": "^1.6",
    "twig/twig": "^2.4"
  },
  "require-dev": {
    "dg/bypass-finals": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Kreyu\\Bundle\\EasyAdminExportBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kreyu\\Bundle\\EasyAdminExportBundle\\Tests\\": "tests"
    }
  }
}
